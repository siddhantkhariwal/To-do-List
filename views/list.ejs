<%- include("header") -%>
    <div class="box" id="heading">
        <h1><%=listTitle%></h1>
    </div>
   <div class="box">
        <%for(var i=0;i<newListItems.length;i++){%>
            <div class="item">
                <input type="checkbox">
                <p><%=newListItems[i] %></p>
            </div>
            
        <%}%>
        <!--this will itereate through the newListItems array and select respective element for each i and put it there-->
        <form class="item" action="/" method="post">
            <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
            <button type="submit" name="list" value=<%= listTitle %>>+</button>
           </form>
   </div>
   <%- include("footer") -%>

   
